    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "gaia",
    "commons",
    "education",
    "regenerative",
    "agriculture",
    "api"
  ],
  "author": "Gaia Commons Initiative",
  "license": "MIT",
  "dependencies": {
    "cors": "2.8.6",
    "dotenv": "16.6.1",
    "express": "4.22.1",
    "express-rate-limit": "7.5.1",
    "helmet": "7.2.0",
    "morgan": "1.10.1",
    "pg": "8.18.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.25",
    "@types/jest": "30.0.0",
    "@types/morgan": "1.9.9",
    "@types/node": "20.19.31",
    "@types/pg": "8.11.0",
    "@types/supertest": "6.0.3",
    "jest": "30.2.0",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
