{
  "name": "gaia-commons-api",
  "version": "5.0.0",
  "description": "Gaia Commons API - Transforming education through regenerative agriculture",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npm run build && npm start",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
